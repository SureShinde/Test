            &:hover .contacthalfmenu {
                display: block;
            }
            &:hover .halfmenu {opacity: 1;visibility: visible;transform: translateY(0);}
        }
        
        > li {
            > .rootmenu-submenu {
                opacity: 0;visibility: hidden; transform: translateY(35px); transition: all .3s ease;
            }
            
            &:hover > .rootmenu-submenu {
                opacity: 1;visibility: visible;transform: translateY(0);
            }
            
            &:hover .megamenu {opacity: 1;visibility: visible;transform: translateY(0);}
            
        }
        
        
        
        .rootmenu-submenu {
            .rootmenu-submenu-sub {
                min-width: 180px;position: absolute;left: 65%;top: 15px;/*display: none !important;*/margin: 0px;padding:0px;border-bottom:solid 2px #097fb4;z-index:99;
                opacity: 0;visibility: hidden; transform: translateY(35px); transition: all .3s ease;
                
                li{
                    &:hover .rootmenu-submenu-sub-sub {
                        /*display: block !important;*/list-style: none;margin: 0px;z-index:100;
                        opacity: 1;visibility: visible;transform: translateY(0);
                    }
                        
                }
                
                .rootmenu-submenu-sub-sub {
                    min-width: 180px;position: absolute;left: 65%;top: 15px;/*display: none !important;*/margin: 0px;padding:0px;border-bottom:solid 2px #097fb4;
                    opacity: 0;visibility: hidden; transform: translateY(35px); transition: all .3s ease;
                    
                    > li {position: relative;padding: 0px;margin: 0px}
                    
                }
                
            }
            
            li{
                &:hover .rootmenu-submenu-sub {/*display: block !important;*/list-style: none;margin: 0px; opacity: 1;visibility: visible;transform: translateY(0);}    
            }
            
        }
        
        
        .mobilelink {
            display: none;
        }
        
        .tabmenu {
            .tabimtag {
                img{max-height:90px; max-width:150px;}
            }
        }
        
        
        .megamenu{
            opacity: 0;visibility: hidden;transform: translateY(35px);transition: all .3s ease;
        }
        
        .halfmenu {
            opacity: 0;visibility: hidden; transform: translateY(35px); transition: all .3s ease;
        }
        

       
 
}
}
    
}

